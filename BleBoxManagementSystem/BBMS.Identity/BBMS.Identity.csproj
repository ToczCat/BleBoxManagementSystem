<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Company>ToczCat</Company>
    <Product>BleBox Management System Component</Product>
    <Copyright>Copyright (C) 2024 Dariusz Garbarz</Copyright>
    <UserSecretsId>4474805a-d89a-4f16-99d7-dce37435683c</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerComposeProjectPath>..\docker-compose.dcproj</DockerComposeProjectPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BCrypt.Net-Next" Version="4.0.3" />
    <PackageReference Include="Grpc.AspNetCore" Version="2.65.0" />
    <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="8.0.2" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.21.0" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="8.0.2" />
    <Protobuf Include="..\..\BBMS.Defaults\Protos\auth.proto" GrpcServices="Server" />
    <Protobuf Include="..\..\BBMS.Defaults\Protos\user.proto" GrpcServices="Client" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\BBMS.Defaults\BBMS.Defaults.csproj" />
  </ItemGroup>

</Project>
